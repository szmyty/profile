<svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="480" height="144" viewBox="0 0 480 144">
  <defs>
    <clipPath id="artwork-clip">
      <rect x="10" y="10" width="100" height="100" rx="8"/>
    </clipPath>
    <linearGradient id="bg-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1a1a2e"/>
      <stop offset="100%" style="stop-color:#16213e"/>
    </linearGradient>
    <filter id="shadow" x="-20%" y="-20%" width="140%" height="140%">
      <feDropShadow dx="0" dy="2" stdDeviation="3" flood-opacity="0.3"/>
    </filter>
  </defs>

  <!-- Background -->
  <rect width="480" height="144" rx="12" fill="url(#bg-gradient)"/>
  <rect width="480" height="144" rx="12" fill="none" stroke="#ff5500" stroke-width="1" stroke-opacity="0.3"/>

  <!-- Artwork -->
  <g clip-path="url(#artwork-clip)">
    <image x="10" y="10" width="100" height="100" preserveAspectRatio="xMidYMid slice" href="data:image/png;base64,"/>
  </g>
  <rect x="10" y="10" width="100" height="100" rx="8" fill="none" stroke="#ff5500" stroke-width="2" stroke-opacity="0.5"/>

  <!-- SoundCloud Logo/Icon -->
  <g transform="translate(370, 10)">
    <circle cx="10" cy="10" r="10" fill="#ff5500"/>
    <text x="10" y="14" font-family="Arial, sans-serif" font-size="10" fill="white" text-anchor="middle" font-weight="bold">SC</text>
  </g>

  <!-- Staleness Badge -->
  <g transform="translate(470, 10)">
    <text x="0" y="12" font-family="'Segoe UI', Arial, sans-serif" font-size="8" fill="#4a5568" text-anchor="end">Updated: just now</text>
  </g>

  <!-- Track Info -->
  <g transform="translate(125, 25)">
    <!-- Title -->
    <text font-family="'Segoe UI', Arial, sans-serif" font-size="16" font-weight="bold" fill="#ffffff">
      starme
    </text>

    <!-- Artist -->
    <text y="22" font-family="'Segoe UI', Arial, sans-serif" font-size="12" fill="#ff5500">
      .play()
    </text>

    <!-- Genre & Duration -->
    <text y="42" font-family="'Segoe UI', Arial, sans-serif" font-size="11" fill="#8892b0">
      <tspan fill="#64ffda">rage cloud bass</tspan>
      <tspan fill="#4a5568"> Â· </tspan>
      <tspan>2:28</tspan>
    </text>

    <!-- Stats -->
    <g transform="translate(0, 58)">
      <!-- Play count icon -->
      <polygon points="0,0 0,10 8,5" fill="#8892b0"/>
      <text x="12" y="9" font-family="'Segoe UI', Arial, sans-serif" font-size="10" fill="#8892b0">144 plays</text>

      <!-- Date -->
      <text x="80" y="9" font-family="'Segoe UI', Arial, sans-serif" font-size="10" fill="#8892b0">Released 2025-12-06 06:29 AM</text>
    </g>
  </g>

  <!-- Clickable overlay (for GitHub markdown) -->
  <a href="https://soundcloud.com/playfunction/starme" target="_blank">
    <rect width="480" height="144" fill="transparent" style="cursor: pointer;"/>
  </a>
</svg>